{
  "name": "nodetest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env PORT=8089 NODE_ENV=development \"npm run lint && ts-node-dev app/index.ts\"",
    "start": "concurrently \"npm run lint && ts-node-dev app/index.ts\" \"cd html && npm run start\"",
    "build": "concurrently \"rimraf dist && npm run lint && tsc -p tsconfig.build.json\" \"cd html && npm run build\"",
    "lint": "./node_modules/.bin/eslint --color --cache --format=node_modules/eslint-formatter-pretty app/**/*.ts"
  },
  "pre-commit": [
    "lint"
  ],
